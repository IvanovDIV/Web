-
    var dictMenu = {
        'Грузовые':'#',
        'Легковые':'#',
        'Список':'#'
    }
    var active_menu = 'Грузовые'
    var titleText = 'Легендарные машины'
mixin createMenu(menu, active)
    each value, key in dictMenu
        if active == key
           .item-a
            a(href= value)= key 
        else
            .item 
                a(href= value)= key
block header
.header
  .header_title
    h3= titleText
  .menu
    +createMenu(dictMenu,active_menu)